<?xml version="1.0"?>
<block>
  <name>PW-Sat2 Frame source</name>
  <key>pwsat2_frame_source</key>
  <category>[PW-Sat2]</category>
  <import>import pwsat2</import>
  <make>pwsat2.pwsat2_frame_source($addr, $isBound)</make>

  <param>
    <name>Address</name>
    <key>addr</key>
    <value></value>
    <type>string</type>
  </param>

  <param>
    <name>Is ZMQ inerface bound</name>
    <key>isBound</key>
    <value>True</value>
    <type>bool</type>
    <hide>part</hide>
  </param>

  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
